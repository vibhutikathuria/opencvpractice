    "import cv2\n",
    "import numpy as np\n",
    "pic=cv2.imread('rdj.jpg')\n",
    "\n",
    "thresholdval1=50\n",
    "thresholdval2=100\n",
    "canny=cv2.Canny(pic,thresholdval1,thresholdval2)\n",
    "\n",
    "cv2.imshow('canny',canny)\n",
    "\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\n"
   ]
